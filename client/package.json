{
  "name": "mini-notes-client",
  "version": "1.0.0",
  "description": "Mini Notes PWA - Client",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:clean": "rm -rf node_modules/.vite && vite",
    "kill": "lsof -ti:5173 | xargs kill -9 || true",
    "start": "npm run kill && npm run dev:clean",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "jsdom": "^28.0.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
